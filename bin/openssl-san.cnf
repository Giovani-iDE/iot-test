[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[ dn ]
C  = PE
ST = Lima
L  = Lima
O  = IOT
OU = server
CN = serveo.net

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = serveo.net
DNS.2 = localhost
IP.1  = 127.0.0.1
